  "devDependencies": {
    "@stryker-mutator/core": "^1.1.0",
    "@stryker-mutator/html-reporter": "^1.1.0",
    "@stryker-mutator/jasmine-runner": "^1.1.0",
    "@stryker-mutator/typescript": "^1.1.0",
    "@types/jasmine": "^3.3.9",
    "@types/node": "^11.11.3",
    "jasmine": "^3.3.1",
    "jasmine-core": "^3.3.0",
    "jasmine-ts": "^0.3.0",
