{
  "name": "intent",
  "version": "0.0.10",
  "description": "Intent language compiler core",
  "main": "build/bin/intent.ts",
  "types": "bin/intent.d.ts",
  "scripts": {
    "executable:alchemy": "chmod +x bin/alchemy/alchemy.ts",
    "executable:intent": "chmod +x bin/intent/intent.ts",
    "intent": "npm run executable:intent && cd bin/intent && ts-node --project src/tsconfig.json -r tsconfig-paths/register intent.ts",
    "alchemy": "TS_NODE_TRANSPILE_ONLY=true ts-node --project bin/alchemy/tsconfig.json ./bin/alchemy/alchemy.ts",
    "pack": "webpack --display-modules --progress --colors",
    "watchdog": "webpack --progress --colors --watch --watch-poll",
    "bump": "bin/bump.sh",
    "test": "jasmine-ts --config=jasmine.json run"
  },
  "keywords": [
    "intent",
    "intent-core"
  ],
  "author": "ankhzet@gmail.com",
  "license": "ISC",
  "dependencies": {
    "chokidar": "^3.5.3",
    "express": "^4.17.3",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "serve-index": "^1.9.1",
    "socket.io": "^4.4.1",
    "util": "^0.12.4",
    "yargs": "^17.4.0"
  },
  "bin": {
    "intent": "bin/alchemy/alchemy.ts"
  },
  "devDependencies": {
    "@stryker-mutator/core": "^5.6.1",
    "@stryker-mutator/html-reporter": "^3.1.0",
    "@stryker-mutator/jasmine-runner": "^5.6.1",
    "@stryker-mutator/typescript": "^4.0.0",
    "@types/jasmine": "^4.0.0",
    "@types/node": "^17.0.23",
    "file-loader": "^0.11.2",
    "jasmine": "^3.0.0",
    "jasmine-core": "^3.0.0",
    "jasmine-ts": "^0.4.0",
    "json-loader": "^0.5.7",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.6.3",
    "webpack": "^5.70.0"
  }
}
